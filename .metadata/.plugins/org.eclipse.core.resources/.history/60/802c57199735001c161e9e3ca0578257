<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
<head>
	<title>cookie验证页面</title>
</head>
<body>
	<%
		String str = "12";
		Cookie cookie1=new Cookie("num", str);
		//服务端增加cookie
		response.addCookie(cookie1);
		//转发cookie给客户端
		response.sendRedirect("result.jsp");
	%>
</body>
</html>