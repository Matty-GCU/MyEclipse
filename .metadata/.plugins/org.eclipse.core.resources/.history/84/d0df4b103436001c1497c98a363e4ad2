    <%
     	Cookie[] cs=request.getCookies();
  		for(int i=0;i<cs.length;i++){
  			if(cs[i].getName().equals("check")&&cs[i].getValue().equals("shi")){
  				Date date=new Date();
  				out.println(date.toString()+"<br>");
  				out.println("您的登录状态已被记录<br>");
  				date=new Date(date.getTime()+60000);
  				out.println("于"+date.toString()+"过期");
  			}
	 	}
	 	 %>
    <title>My JSP 'zuoye5_3.jsp' starting page</title>
    
