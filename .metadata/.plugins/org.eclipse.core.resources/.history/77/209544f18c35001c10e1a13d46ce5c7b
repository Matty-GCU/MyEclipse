<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<html>
	<head>
		<title>Insert title here</title>
	</head>
	<body>
		<%
			String str = null;
			Cookie[] cookies = request.getCookies();
			for(Cookie c: cookies) {
				if(c.getName().equals("num")) {
					str = c.getValue();
					break;
				}
			}
		%>
		<%= str%>
		<%		
			int num = Integer.parseInt(str);
		%>
		该数字的立方：<%= num * num * num%>
	</body>
</html>